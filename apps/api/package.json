{
  "name": "@d4mr/0list-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "types": "./dist/db/schema.d.ts"
    },
    "./types": {
      "types": "./dist/bindings.d.ts"
    }
  },
  "scripts": {
    "dev": "wrangler dev",
    "build": "tsc",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 execute 0list-db --local --file=./drizzle/0000_huge_preak.sql",
    "db:migrate:remote": "wrangler d1 execute 0list-db --remote --file=./drizzle/0000_huge_preak.sql",
    "db:studio": "drizzle-kit studio",
    "seed:generate": "bun run scripts/seed-demo.ts > scripts/seed.sql",
    "seed:local": "bun run seed:generate && wrangler d1 execute 0list-db --local --file=./scripts/seed.sql",
    "seed:remote": "bun run seed:generate && wrangler d1 execute 0list-db --remote --file=./scripts/seed.sql"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.6",
    "@react-email/components": "^1.0.4",
    "@react-email/render": "^2.0.2",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.3",
    "jose": "^6.1.3",
    "mimetext": "^3.0.27",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.7.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260109.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.9.3",
    "wrangler": "^4.58.0"
  }
}
